{
  "name": "solana-self-sniper",
  "version": "0.1.0",
  "description": "Solana self-sniper bot that creates Raydium liquidity pool and mirrors buys.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "bundle": "tsx src/bundle.ts",
    "watch": "tsx src/watch.ts",
    "bundle-and-watch": "npm-run-all --serial bundle watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "vitest run"
  },
  "dependencies": {
    "@solana/web3.js": "^2.0.0",
    "@raydium-io/raydium-sdk": "latest",
    "@jito-foundation/jito-ts": "latest",
    "commander": "^10.0.0",
    "dotenv": "^16.3.1",
    "tsx": "^3.12.7",
    "winston": "^3.12.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "prettier": "^3.0.0",
    "vitest": "^0.34.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "npm-run-all": "^4.1.5",
    "concurrently": "^8.2.0",
    "ts-mockito": "^2.6.1",
    "@typescript-eslint/parser": "^6.9.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0"
  },
  "engines": {
    "node": ">=20"
  },
  "license": "MIT"
} 